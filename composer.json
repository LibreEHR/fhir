{
    "name": "libre-ehr/fhir",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "LibreEHR",
        "LibreEHR on FHIR"
    ],
    "homepage": "https://github.com/:vendor/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": "Ken Chapple",
            "email": "ken@mi-squared.com",
            "homepage": "http://mi-squared.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "libre-ehr/core": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "LibreEHR\\FHIR\\": "src",
            "LibreEHR\\Core\\": "vendor/libre-ehr/core/src",
            "PHPFHIRGenerated\\": "src/PHPFHIRGenerated"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LibreEHR\\FHIR\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
